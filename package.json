{
  "name": "gatsby-source-openapi-aggregate",
  "version": "0.1.7",
  "description":
    "Gatsby source plugin for pulling data into Gatsby from Open API/Swagger specifications",
  "scripts": {
    "build": "babel ./src --out-dir . --ignore __tests__",
    "watch": "babel ./src -w . --out-dir . --ignore __tests__",
    "test": "jest",
    "test:watch": "jest --verbose --watchAll ",
    "release": "standard-version"
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", ".cache"],
    "verbose": true
  },
  "keywords": ["gatsby", "gatsby-plugin", "gatsby-source-plugin"],
  "author": "DevDigital <devdigital.cloud@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "2.1.0",
    "lodash.omit": "4.5.0",
    "inspected": "0.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "5.1.4",
    "jest": "21.2.1",
    "standard-version": "4.2.0"
  }
}
